{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }, {
        "source": "/dogs/:name", 
        "destination": "/dogs/:name",  
        "type": 301
      }
    ]
  }
}
